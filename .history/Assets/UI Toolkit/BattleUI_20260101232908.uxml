                    <ui:Label text="← Back" name="BackButton" class="font back-button" style="font-size: 18px;"/>
                    <ui:Label text="Items" name="PageTitle" class="font" style="font-size: 20px; -unity-text-align: upper-center; flex-grow: 1;"/>
                </ui:VisualElement>
                <ui:VisualElement name="ItemList" style="flex-grow: 1;">
                    <ui:Label text="Item list would go here" class="font" style="margin-top: 50px; -unity-text-align: upper-center;"/>
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="FleePage" style="flex-grow: 1; display: none;">
                <ui:VisualElement style="flex-direction: row; margin-bottom: 10px; flex-shrink: 0;">
