                    <ui:Label text="Attack Skills" name="PageTitle" class="font" style="font-size: 20px; -unity-text-align: upper-center; flex-grow: 1;"/>
                </ui:VisualElement>
                <ui:VisualElement name="AttackSkillList" style="flex-grow: 1; flex-direction: column;">
                    <ui:VisualElement name="Skill1" class="skill-item">
                        <ui:VisualElement name="SkillIcon" class="skill-icon" style="background-image: url(&quot;project://database/Assets/PlaceHolderArt/Skulls/basicSkullGrey.png?fileID=6123531346467591110&amp;guid=30547456e443cc1439211476fb2c46b6&amp;type=3#basicSkullGrey_0&quot;); -unity-background-scale-mode: scale-to-fit;"/>
