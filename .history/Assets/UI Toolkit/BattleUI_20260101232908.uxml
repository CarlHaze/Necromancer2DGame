                <ui:Label text="Rattlebones" name="CreatureName" class="font creature-name" style="-unity-text-align: middle-center; margin-bottom: 5px;"/>
                <ui:VisualElement name="Portrait" class="portrait" style="background-image: url(&quot;project://database/Assets/PlaceHolderArt/Skulls/basicSkullGrey.png?fileID=6123531346467591110&amp;guid=30547456e443cc1439211476fb2c46b6&amp;type=3#basicSkullGrey_0&quot;); -unity-background-scale-mode: scale-to-fit;"/>
                <ui:Label text="Level: 12" name="Level" class="font level-text" style="margin-left: 5px;"/>
                <ui:Label text="HP: 45/60" name="HPText" class="font stat-text" style="margin-left: 5px;"/>
                <ui:VisualElement name="HPBar" class="stat-bar hp-bar">
                    <ui:VisualElement name="HPFill" class="bar-fill hp-fill" style="width: 75%;"/>
                </ui:VisualElement>
                <ui:Label text="SP: 30/60" name="SPText" class="font stat-text" style="margin-left: 5px;"/>
