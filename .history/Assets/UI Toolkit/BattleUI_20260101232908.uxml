        <ui:VisualElement name="GrimoireContainer" style="position: absolute; bottom: 0; left: 50%; translate: -50% 0; width: 500px; height: 400px; background-color: rgba(0, 0, 0, 0.85); border-radius: 5px; padding: 10px; border-width: 2px; border-color: rgba(100, 100, 100, 0.8); margin-bottom: 5px;">
