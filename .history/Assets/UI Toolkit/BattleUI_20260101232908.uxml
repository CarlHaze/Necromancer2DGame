        <ui:VisualElement name="BattleLogContainer" style="position: absolute; bottom: 0; right: 20px; width: 580px; height: 250px; background-color: rgba(0, 0, 0, 0.85); border-radius: 5px; padding: 10px; border-width: 2px; border-color: rgba(100, 100, 100, 0.8); margin-bottom: 5px;">
