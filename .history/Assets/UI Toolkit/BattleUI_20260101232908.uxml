                <ui:VisualElement name="Portrait" class="portrait" style="background-image: url(&quot;project://database/Assets/PlaceHolderArt/Skulls/basicSkullBlue.png?fileID=1546111035941289287&amp;guid=6c663173e13a9bd4fbd9e9a10032e1fd&amp;type=3#basicSkullBlue_0&quot;); -unity-background-scale-mode: scale-to-fit;"/>
                <ui:Label text="Level: 11" name="Level" class="font level-text" style="margin-left: 5px;"/>
                <ui:Label text="HP: 45/60" name="HPText" class="font stat-text" style="margin-left: 5px;"/>
                <ui:VisualElement name="HPBar" class="stat-bar hp-bar">
