                    <ui:Label text="Summon Creature" name="PageTitle" class="font" style="font-size: 20px; -unity-text-align: upper-center; flex-grow: 1;"/>
                </ui:VisualElement>
                <ui:VisualElement name="SummonList" style="flex-grow: 1;">
                    <ui:Label text="Summon list would go here" class="font" style="margin-top: 50px; -unity-text-align: upper-center;"/>
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="ItemPage" style="flex-grow: 1; display: none;">
