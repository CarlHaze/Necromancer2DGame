                    <ui:Label text="Cast smoke screen to flee from battle?" class="font" style="font-size: 18px; -unity-text-align: upper-center; margin-bottom: 20px;"/>
                    <ui:VisualElement name="ConfirmFlee" class="skill-item">
                        <ui:Label text="☁ Cast Escape Spell" class="font" style="font-size: 20px; flex-grow: 1; -unity-text-align: middle-center;"/>
