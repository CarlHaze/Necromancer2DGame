                    </ui:VisualElement>
                    <ui:VisualElement name="Skill2" class="skill-item">
                        <ui:VisualElement name="SkillIcon" class="skill-icon" style="background-image: url(&quot;project://database/Assets/PlaceHolderArt/Skulls/basicSkullBlue.png?fileID=1546111035941289287&amp;guid=6c663173e13a9bd4fbd9e9a10032e1fd&amp;type=3#basicSkullBlue_0&quot;); -unity-background-scale-mode: scale-to-fit;"/>
