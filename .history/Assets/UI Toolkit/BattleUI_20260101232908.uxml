                <ui:Label text="SP: 30/60" name="SPText" class="font stat-text" style="margin-left: 5px;"/>
                <ui:VisualElement name="SPBar" class="stat-bar sp-bar">
                    <ui:VisualElement name="SPFill" class="bar-fill sp-fill" style="width: 50%;"/>
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="Party3Container" class="party-container" style="background-color: rgba(0, 0, 0, 0.85); margin: 5px;">
                <ui:Label text="Grave Stalker" name="CreatureName" class="font creature-name" style="-unity-text-align: middle-center; margin-bottom: 5px;"/>
                <ui:VisualElement name="Portrait" class="portrait" style="background-image: url(&quot;project://database/Assets/PlaceHolderArt/Skulls/basicSkullRed.png?fileID=-5289296835903173981&amp;guid=67bffca3c921f2c4cae78485a83247bc&amp;type=3#basicSkullRed_0&quot;); -unity-background-scale-mode: scale-to-fit;"/>
