                    <ui:Label text="Enemy Ghost took 15 damage!" class="font log-message log-damage" style="margin-bottom: 3px;"/>
                    <ui:Label text="Enemy Ghost used Shadow Claw!" class="font log-message log-action" style="margin-bottom: 3px;"/>
                    <ui:Label text="Rattlebones took 12 damage!" class="font log-message log-damage" style="margin-bottom: 3px;"/>
                    <ui:Label text="Rot Walker used Defend!" class="font log-message log-action" style="margin-bottom: 3px;"/>
                    <ui:Label text="Rot Walker&apos;s defense increased!" class="font log-message log-status" style="margin-bottom: 3px;"/>
