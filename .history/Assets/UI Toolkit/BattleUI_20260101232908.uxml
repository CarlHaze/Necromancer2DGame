                        <ui:Label text="[Dark]" name="SkillType" class="font skill-type"/>
                        <ui:Label text="6 SP" name="SkillCost" class="font skill-cost"/>
                    </ui:VisualElement>
                    <ui:VisualElement name="Skill3" class="skill-item">
                        <ui:VisualElement name="SkillIcon" class="skill-icon" style="background-image: url(&quot;project://database/Assets/PlaceHolderArt/Skulls/basicSkullRed.png?fileID=-5289296835903173981&amp;guid=67bffca3c921f2c4cae78485a83247bc&amp;type=3#basicSkullRed_0&quot;); -unity-background-scale-mode: scale-to-fit;"/>
