<ui:UXML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI%20Toolkit/BattleUIStyle.uss?fileID=7433441132597879392&amp;guid=36517001232700b409a06fd4941b512e&amp;type=3#BattleUIStyle"/>
    <ui:VisualElement name="Container" style="flex-grow: 1; position: relative;">
        <ui:VisualElement name="TurnOrderContainer" style="flex-grow: 1; align-self: center; width: 50%; height: 80px; position: absolute; top: 0; background-color: rgba(0, 0, 0, 0.59); padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; margin-top: 20px; border-top-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px; border-bottom-left-radius: 10px;">
            <ui:VisualElement name="Portrait" class="portrait" style="flex-grow: 1; width: 60px; height: 60px; background-color: rgb(255, 255, 255); margin-top: 5px; margin-right: 5px; margin-bottom: 5px; margin-left: 5px; border-top-left-radius: 10px; border-top-right-radius: 10px; border-bottom-right-radius: 10px; border-bottom-left-radius: 10px; border-top-width: 2px; border-right-width: 2px; border-bottom-width: 2px; border-left-width: 2px; border-left-color: rgb(25, 43, 212); border-right-color: rgb(25, 43, 212); border-top-color: rgb(25, 43, 212); border-bottom-color: rgb(25, 43, 212);"/>
        </ui:VisualElement>
        <ui:VisualElement name="BottomUIContainer" style="flex-grow: 0; bottom: 0; position: absolute; width: 600px; height: 300px; left: 20px; flex-direction: row; justify-content: space-between;">
